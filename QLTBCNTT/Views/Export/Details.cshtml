@model QLTBCNTT.Models.PhieuXuat
@{
    ViewData["Title"] = "Chi tiết phiếu xuất";
}

<h2>@ViewData["Title"]</h2>

<div>
    <h4>Phiếu xuất</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">Mã phiếu xuất</dt>
        <dd class="col-sm-10">@Model.MaPhieuxuat</dd>

        <dt class="col-sm-2">Ngày xuất</dt>
        <dd class="col-sm-10">@Model.NgayXuat</dd>

        <dt class="col-sm-2">Phòng ban</dt>
        <dd class="col-sm-10">@Model.MaPhongbanNavigation?.TenPhongban</dd>

        <dt class="col-sm-2">Người nhận</dt>
        <dd class="col-sm-10">@Model.NguoiNhanNavigation?.HoTen</dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.MaPhieuxuat">Sửa</a> |
    <a asp-action="Index">Quay lại danh sách</a>
</div>